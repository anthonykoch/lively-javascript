{
  "private": true,
  "name": "lively-javascript",
  "version": "1.0.0",
  "description": "Execution and tracking of javascript expressions",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "test": "ava test/*.test.js",
    "build": "npm run clean && npx babel lib --out-dir dist --ignore lib/testing.js",
    "build-browser": "rm -rf build/* && webpack --config webpack.config.js -p",
    "setup": "yarn install --only dev"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/generator": "7.0.0-beta.40",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-beta.40",
    "@babel/polyfill": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/traverse": "7.0.0-beta.40",
    "babel-loader": "8.0.0-beta.0",
    "bowser": "1.8.1",
    "lodash": "4.17.4",
    "source-map": "0.7.2",
    "webpack": "4.1.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.10"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "@babel/register": "7.0.0-beta.40",
    "@babel/cli": "7.0.0-beta.40",
    "eslint": "4.9.0"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "browserslist": [
    "ie >= 9"
  ],
  "author": "Anthony Koch",
  "license": "MIT"
}
