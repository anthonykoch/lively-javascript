{
  "private": true,
  "name": "lively-javascript",
  "version": "1.0.0",
  "description": "Execution and tracking of javascript expressions",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint --quiet -c .eslintrc.yml **/*.js ",
    "clean": "rm -rf dist/*",
    "test": "ava test/*.test.js",
    "test:watch": "npm run test -- --watch",
    "build": "npm run clean && npx babel lib --out-dir dist --ignore lib/testing.js",
    "build-browser": "rm -rf build/* && webpack --config webpack.config.js -p",
    "cov": "npx nyc --reporter=lcov --require @babel/register npm run test",
    "setup": "npm install --only dev"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/generator": "7.0.0-beta.40",
    "@babel/polyfill": "7.0.0-beta.40",
    "@babel/runtime": "7.0.0-beta.40",
    "@babel/traverse": "7.0.0-beta.40",
    "check-types": "7.3.0",
    "lodash": "4.17.4",
    "source-map": "0.7.2"
  },
  "devDependencies": {
    "webpack": "4.1.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.10",
    "babel-eslint": "8.2.2",
    "babel-loader": "8.0.0-beta.0",
    "@babel/cli": "7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "7.0.0-beta.40",
    "@babel/register": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-beta.40",
    "ava": "0.25.0",
    "eslint": "4.9.0",
    "glob": "7.1.2",
    "nyc": "11.4.1"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "browserslist": [
    "ie >= 9"
  ],
  "author": "Anthony Koch",
  "license": "MIT"
}
